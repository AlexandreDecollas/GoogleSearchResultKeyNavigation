{
    "manifest_version": 2,
    "name": "Instant search shortcut",
    "description": "Restores some of the keyboard shortcuts available in instant search",
    "version": "0.1.0",
  //"default_locale": "en",
    "author": "Kirens",
    "icons": {
        "16":  "res/icon16.png",
        "48":  "res/icon48.png",
        "128": "res/icon128.png",
        "256": "res/icon256.png"
    },

    "content_scripts": [
        {
            "matches":["*://www.google.se/search*"],
            "js": ["instant.js"],
            "css": ["instant.css"],
            "run_at": "document_start"
        }
    ], "web_accessible_resources": [
        "essentials.css"
    ], "permissions": [
        "https://www.google.se/*"
    ]
}
