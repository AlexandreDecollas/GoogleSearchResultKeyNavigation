{
    "manifest_version": 2,
    "name": "Google Search Result Key Navigation",
    "description": "Navigate throw google search results with keys",
    "version": "1.0.0",
    "author": "Alexandre Decollas",
    "icons": {
        "16":  "assets/icon16.png",
        "48":  "assets/icon48.png",
        "128": "assets/icon128.png",
        "256": "assets/icon256.png"
    },

    "content_scripts": [
        {
            "matches":[
                "*://www.google.com/search*",
                "*://www.google.se/search*"
            ],
            "js": ["google-search-result-key-navigation.js", "utils.js"],
            "css": ["google-search-result-key-navigation.css"],
            "run_at": "document_start"
        }
    ], "web_accessible_resources": [
        "essentials.css"
    ], "permissions": [
        "https://www.google.se/*"
    ]
}
